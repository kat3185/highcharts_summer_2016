<div id="absurd-graph" style="width:100%; height:400px;"></div>
<div class="large-4 columns">
  <table>
    <tr>
      <th>Year</th>
      <th>Winning Word Length</th>
      <th>Venomous Spider Deaths</th>
    </tr>
    <% @data.each do |row| %>
      <tr>
        <td><%= row[0] %></td>
        <td><%= row[1] %></td>
        <td><%= row[2] %></td>
      </tr>
    <% end %>
  </table>
</div>
<br>
